pipeline{
    agent any
    stages{
        stage('Roy'){
            steps{
                sh 'pwd'
                echo "action1"
            }
        }
        stage('Rogers'){
            steps{
                sh 'lsblk'
                echo "action2"
            }
        }
        stage('Silas'){
            steps{
                sh 'whoami'
                echo "action3"
            }
        }
        stage('Warami'){
            steps{
                sh 'df -h'
                echo "action4"
            }
        }
        stage('Abiola'){
            steps{
                sh 'free -g'
                echo "action5"
            }
        }
    }
}